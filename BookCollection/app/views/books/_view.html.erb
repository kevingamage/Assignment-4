<table summary="Book detail view">
  <tr>
    <th>Title</th>
    <td> <%= @book.title %> </td>
  </tr>
  <tr>
    <th>Author</th>
    <td> <%= @book.author %> </td>
  </tr>
  <tr>
    <th>Genre</th>
    <td> <%= @book.genre %> </td>
  </tr>
  <tr>
      <th>Price</th>
      <td> <%= @book.price %> </td>
  </tr>
    <th>Published Date</th>
    <td> <%= @book.publish_date %> </td>
  </tr>
</table>
